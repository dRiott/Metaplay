$(document).ready(function () {
	
	for(var i = $("#artistsTable tr").length; i-=1;) {
		$("#showButton"+i).click(function () {
			console.log("Button id: " + $(this).attr('id'));
			console.log("Looking for the span's id: " + $(this).closest('td').html());
			//find('span')
		})
		
		console.log($("#containsBio"+i).next().css('border', 'dashed 2px'));
		
		console.log($("#containsBio"+i).find("#showButton"+i).css('background-color', 'red'))
	}
	
	
