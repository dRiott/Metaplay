alert("HEY!");

function validatePassword () {
	var password1 = document.getElementById("account-password").value;
	var password2 = document.getElementById("account-passwordConfirm").value;
	var mainRow = document.getElementById("mainRow");

	
	if(password1!==password2) {
		alert("inside if");
		var node = document.getElementById('mainRow');
		var newNode = document.createElement('p');
		newNode.appendChild(document.createTextNode('some dynamic html'));
		node.appendChild(newNode);
		return false;

	} else {
		return true;
	}
}
